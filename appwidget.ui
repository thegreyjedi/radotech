   <widget class="QWidget" name="historyPage">
    <property name="styleSheet">
     <string notr="true">/* History Tab - ListWidget Styling */
QListWidget {
    background: transparent; /* Transparent background for the list */
    border: 2px solid rgba(255, 120, 0, 255); /* Orange border around the list */
    border-radius: 12px; /* Rounded corners for the entire list */
    padding: 10px; /* Padding inside the list */
    color: white; /* White text */
    font-size: 16px; /* Font size for items */
}

/* List Item Styling */
QListWidget::item {
    background: rgba(0, 0, 0, 0.15); /* Darker background for non-selected items */
    border: none;
    color: white; /* White text color */
    padding: 8px 12px; /* Padding around items */
    border-radius: 10px; /* Rounded corners for each item */
    margin: 5px 0; /* Space between items */
}

/* Selected Item Styling */
QListWidget::item:selected {
    background-color: rgba(255, 120, 0, 0.8); /* Darker orange background when selected */
    border-radius: 10px; /* Rounded corners for selected items */
}

/* Hover Effect for List Items */
QListWidget::item:hover {
    background-color: rgba(255, 120, 0, 0.3); /* Light orange background on hover */
    border-radius: 10px; /* Rounded corners for hover effect */
}

/* History Tab - Button Styling */
QPushButton {
    background: qlineargradient(
        spread: pad, 
        x1: 1, y1: 0.045, 
        x2: 1, y2: 1, 
        stop: 0 rgba(255, 120, 0, 255), 
        stop: 1 rgba(246, 97, 81, 255)
    ); /* Gradient background like sidebar */
    color: white; /* White text */
    border: none; /* No border */
    border-radius: 12px; /* Rounded corners for button */
    font-size: 16px;
    padding: 10px 20px; /* Padding for the button */
    text-align: center; /* Centered text */
}

/* Button Hover Effect */
QPushButton:hover {
    border: 1px solid rgba(255, 255, 255, 0.6); /* Light border on hover */
    background: rgba(255, 120, 0, 180); /* Slightly darker when hovering */
}

