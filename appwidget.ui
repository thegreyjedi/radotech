    <property name="styleSheet">
     <string notr="true">/* GroupBox */
QGroupBox {
    background: transparent; /* Transparent background */
    border: 2px solid rgba(255, 120, 0, 255); /* Border to match the sidebar */
    border-radius: 8px; /* Rounded corners */
    padding: 10px; /* Padding around content */
    margin: 10px; /* Margin to separate from other content */
    font-size: 18px;
    color: white; /* White text color for visibility */
}

/* GroupBox title */
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding-left: 10px;
    padding-top: 5px;
    color: white; /* Title color */
}

/* Radio Buttons inside GroupBox */
QRadioButton {
    background: transparent; /* Transparent background */
    color: white;             /* White text for visibility */
    border: none;             /* No border by default */
    font-size: 16px;          /* Adjust font size */
    padding: 5px;             /* Add padding around text */
}

/* Hover effect for Radio Buttons */
QRadioButton:hover {
    border: 1px solid rgba(255, 255, 255, 0.4); /* Light border on hover */
    border-radius: 4px;                          /* Rounded corners */
}

/* When Radio Button is selected */
QRadioButton:checked {
    color: #ff7f00; /* Change text color when checked */
}

/* Radio Button indicator (round) */
QRadioButton::indicator {
    border: 2px solid #ff7f00; /* Orange border */
    border-radius: 10px;        /* Round border */
    width: 16px;                /* Size of the circle */
    height: 16px;               /* Size of the circle */
    background-color: transparent; /* Transparent background */
}

/* When Radio Button is checked, change the indicator */
QRadioButton::indicator:checked {
    background-color: #ff7f00;  /* Orange when selected */
    border-color: #ff7f00;      /* Orange border when selected */
}

/* Styling for QPushButtons in homePage Widget */
QPushButton {
    background: qlineargradient(
        spread: pad, 
        x1: 1, y1: 0.045, 
        x2: 1, y2: 1, 
        stop: 0 rgba(255, 120, 0, 255), 
        stop: 1 rgba(246, 97, 81, 255)
    ); /* Gradient background like sidebar */
    color: white; /* White text */
    border: none; /* No border */
    border-radius: 12px; /* Rounded corners */
    font-size: 16px;
    padding: 10px 20px; /* Adjust padding for button size */
    text-align: center; /* Centered text */
}

/* Hover effect for buttons */
QPushButton:hover {
    border: 1px solid rgba(255, 255, 255, 0.6); /* Light border on hover */
    background: rgba(255, 120, 0, 180); /* Slightly darker when hovering */
}

/* Button Pressed Effect */
QPushButton:pressed {
    background: rgba(255, 255, 255, 0.2); /* Subtle background change */
    background-color: rgba(255, 120, 0, 220); /* Darker orange when pressed */
}
</string>
